package ch02.sec01;

public class VarEcEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 3;
		int y = 5;
		System.out.println("x:" + x + ", y:" + y);
		
		int temp = x; // temp = 3
		x = y; // x = 5
		y = temp; // y = 3
		System.out.println("x:" + x + ", y:" + y);

	}//end main()
	
	void swap(int x, int y) {
		int temp = y; // temp = 5
		y = x; // y = 3
		x = temp; // x = 5
	}

}//end class
